{
  "name": "@alibe/data-geocodes",
  "version": "1.0.0",
  "description": "Data builder for the data utilized by the geoCodes apps",
  "main": "build.js",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "node build",
    "lint": "eslint .",
    "test": "jest --verbose",
    "start": "npm-run-all --sequential lint build test"
  },
  "keywords": [
    "geocodes",
    "data",
    "builder"
  ],
  "author": {
    "name": "Andrea Liberati",
    "url": "https://alibe.ie"
  },
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alibe1971/data-geocodes.git"
  },
  "bugs": {
    "url": "https://github.com/alibe1971/data-geocodes/issues"
  },
  "homepage": "https://github.com/alibe1971/data-geocodes#readme",
  "documentation": [
    {
      "type": "Alibe Docs",
      "url": "https://url-documentazione1"
    },
    {
      "type": "Netifly",
      "url": "https://altro-url"
    }
  ],
  "dependencies": {
    "chalk": "^5.3.0",
    "slugify": "^1.6.6",
    "svgo": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.24.7",
    "babel-jest": "^29.7.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "xml2js": "^0.6.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "testMatch": [
      "<rootDir>/Tests/**/?(*.)+(spec|test).js"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "<rootDir>",
          "outputName": "junit.xml"
        }
      ]
    ],
    "maxWorkers": 2,
    "transformIgnorePatterns": [],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
